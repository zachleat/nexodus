{
  "name": "@zachleat/nexodus",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "import-bsky": "npx @11ty/import bluesky @zachleat.com --output=bluesky --cacheduration=4h",
    "import-fedi": "npx @11ty/import fediverse zachleat@fediverse.zachleat.com --output=fediverse --cacheduration=4h",
    "import": "npm run import-bsky && npm run import-fedi",
    "import-prod-bsky": "npx @11ty/import bluesky @zachleat.com --output=bluesky --cacheduration=4h --persist=github:zachleat/nexodus",
    "import-prod-fedi": "npx @11ty/import fediverse zachleat@fediverse.zachleat.com --output=fediverse --cacheduration=4h --persist=github:zachleat/nexodus",
    "import-production": "npm run import-prod-bsky && npm run import-prod-fedi",
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve",
    "production": "npm run import-production && npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/import": "^1.0.0-beta.1"
  }
}
